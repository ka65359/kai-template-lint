{
  "name": "kai-template-lint",
  "version": "1.0.0",
  "description": "Kai's Project Template with linting and formatting on commit",
  "repository": {
    "type": "git",
    "url": "https://github.com/ka65359/kai-template-lint.git"
  },
  "author": "Kai Arrowood",
  "license": "MIT",
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "^3.4.0"
  },
  "scripts": {
    "build": "react-scripts build",
    "clean": "yarn cache clean && npm install",
    "lint": "eslint ./src",
    "scripts": "npm-scripts-info",
    "start": "react-scripts start",
    "start:dev": "run-p start:server start",
    "start:server": "node-env-run server --exec nodemon | pino-colada",
    "test": "react-scripts test",
    "validate": "yarn check --integrity"
  },
  "scripts-info": {
    "scripts": "Display information about the scripts",
    "start": "Start the application",
    "start:server": "Start the server",
    "start:dev": "Start the application and the server",
    "lint": "Check for linter errors",
    "validate": "Check for improperly installed dependencies"
  },
  "proxy": "http://localhost:4001",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "body-parser": "^1.19.0",
    "eslint": "^6.8.0",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "21.18.0",
    "eslint-plugin-react": "7.10.0",
    "express": "^4.17.1",
    "express-pino-logger": "^4.0.0",
    "node-env-run": "^3.0.2",
    "nodemon": "^1.19.3",
    "npm-run-all": "^4.1.5",
    "npm-scripts-info": "^0.3.6",
    "pino-colada": "^1.4.5"
  }
}
